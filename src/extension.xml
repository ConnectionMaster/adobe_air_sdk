<extension xmlns="http://ns.adobe.com/air/extension/51.1">
    <id>com.adjust.sdk</id>
    <versionNumber>5.0.0</versionNumber>
    <platforms>
        <platform name="Android-ARM">
            <applicationDeployment>
                <nativeLibrary>adjust-android.aar</nativeLibrary>
                <initializer>com.adjust.sdk.AdjustExtension</initializer>
            </applicationDeployment>
        </platform>
        <platform name="Android-ARM64">
            <applicationDeployment>
                <nativeLibrary>adjust-android.aar</nativeLibrary>
                <initializer>com.adjust.sdk.AdjustExtension</initializer>
            </applicationDeployment>
        </platform>
        <platform name="Android-x86">
            <applicationDeployment>
                <nativeLibrary>adjust-android.aar</nativeLibrary>
                <initializer>com.adjust.sdk.AdjustExtension</initializer>
            </applicationDeployment>
        </platform>

        <platform name="iPhone-ARM">
            <applicationDeployment>
                <nativeLibrary>libAdjustExtension.a</nativeLibrary>
                <initializer>AdjustFREInitializer</initializer>
                <finalizer>AdjustFREFinalizer</finalizer>
            </applicationDeployment>
        </platform>
        <platform name="iPhone-x86">
            <applicationDeployment>
                <nativeLibrary>libAdjustExtension.a</nativeLibrary>
                <initializer>AdjustFREInitializer</initializer>
                <finalizer>AdjustFREFinalizer</finalizer>
            </applicationDeployment>
        </platform>

        <platform name="default">
            <applicationDeployment/>
        </platform>
    </platforms>
</extension>
